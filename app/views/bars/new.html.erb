<center>
  <%= form_with url: '/bars', method: :post, local: true do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :minimum_age %>
    <%= f.number_field :minimum_age %>
    <%= f.label :rating %>
    <%= f.number_field :rating %>
    <%= f.submit 'Create Bar' %>
  <% end %>
</center>